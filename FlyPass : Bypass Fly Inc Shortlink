var flid = getCookie("flid");

function getCookie(cname) {
  var name = cname + "=";
  var decodedCookie = decodeURIComponent(document.cookie);
  var ca = decodedCookie.split(';');
  for(var i = 0; i <ca.length; i++) {
    var c = ca[i];
    while (c.charAt(0) == ' ') {
      c = c.substring(1);
    }
    if (c.indexOf(name) == 0) {
      return c.substring(name.length, c.length);
    }
  }
  return "";
}

if(window.location.href.indexOf("get") > -1) {
window.location = "https://anhautobot.com/api/bypass/?name=https://anhautobot.com/api/bypass/api/?url=" + window.location.href + "&timer=" + BypassTimer
}
else {
    window.location = "https://anhautobot.com/api/bypass/?name=https://anhautobot.com/api/bypass/api/?url=https://" + window.location.hostname + "/backup/w/?get=" + flid + "&timer=" + BypassTimer
}
